package com.kcatalog.common;

import java.util.HashMap;

public abstract class KCatalogDto {
	private static HashMap errorMap = new HashMap();
	
	public static void addToErrorMap(String messageId, String additionalInfo ){
		
		errorMap.put(messageId,additionalInfo);
		
	}
	
	public static HashMap getErrorMap(){
		return errorMap;
	}
}
